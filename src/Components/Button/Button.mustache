<a href="{{url}}" class="{{getRootClasses}}" {{target}}>
  {{#iconHtml}}
    <div class="icon">
      {{{iconHtml}}}
    </div>
  {{/iconHtml}}
  <div class="labels">
    {{#label}}
      <div class="label">{{label}}</div>
    {{/label}}
    {{#sublabel}}
      <div class="sublabel">{{sublabel}}</div>
    {{/sublabel}}
  </div>
</a>
